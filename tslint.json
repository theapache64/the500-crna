{
  "extends": [
    "tslint-config-airbnb",
    "tslint-react"
  ],
  "rules": {
    // override tslint-react rules here
    // These are just dev-rules
    "no-trailing-whitespace": false,
    // Don't touch below rules
    "jsx-wrap-multiline": false,
    "variable-name": false,
    "no-default-export": true,
    "trailing-comma": false
  }
}